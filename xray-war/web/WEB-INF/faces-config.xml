<?xml version='1.0' encoding='UTF-8'?>
<faces-config 
    version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
>
    <application>
        <locale-config>
            <default-locale>en</default-locale>
            <supported-locale>de</supported-locale>
        </locale-config>
        <resource-bundle>
            <base-name>jee18.i18n.Messages</base-name>
            <var>msg</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>sign-in</from-outcome>
            <to-view-id>/app/sign-in.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>sign-out</from-outcome>
            <to-view-id>/app/sign-in.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/app/home.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>person-list</from-outcome>
            <to-view-id>/person/person-list.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>person-form</from-outcome>
            <to-view-id>/person/person-form.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>contract-list</from-outcome>
            <to-view-id>/contract/contract-list.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>contract-form</from-outcome>
            <to-view-id>/contract/contract-form.xhtml</to-view-id>
        </navigation-case>
            <navigation-case>
            <from-outcome>contract-stats</from-outcome>
            <to-view-id>/contract/contract-stats.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>timesheet-list</from-outcome>
            <to-view-id>/timesheet/timesheet-list.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>timesheet-form</from-outcome>
            <to-view-id>/timesheet/timesheet-form.xhtml</to-view-id>
        </navigation-case>
    
        
        <navigation-case>
            <from-outcome>timesheet-entry-list</from-outcome>
            <to-view-id>/timesheet-entry/timesheet-entry-list.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>timesheet-entry-form</from-outcome>
            <to-view-id>/timesheet-entry/timesheet-entry-form.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
